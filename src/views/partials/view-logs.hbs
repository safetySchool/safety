{{#extend "css"}}
    <link rel="stylesheet" type="text/css" href="/public/app/logs/view-logs/css/style.css">
{{/extend}}
<div class="modal fade" id="viewLogs-modal">
    <div class="modal-dialog modal-xl">
        <div class="modal-content">
            <div class="modal-body">
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                        <div class="panel-body text-left">
                            <legend>
                                <h4 class="modal-title"><i class="fas fa-list fa-lg"></i>
                                    codePACS / Registro LOGS
                                </h4>
                                <label id="logs-report-number"></label>
                            </legend>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-12 col-md-12 col-sm-12">
                            <ul id="list-all-logs" class="list-group">
                                <!-- controller.getAllLogs() -->
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <br>
                <button type="button" class="btn btn-danger" data-dismiss="modal">Cerrar</button>
            </div>
        </div>
    </div>
</div>


{{#extend "js"}}
    <script src="/public/app/logs/view-logs/js/ControllerViewLogs.js"></script>
    <script src="/public/app/logs/view-logs/js/mainViewLogs.js"></script>
    <script src="/public/app/logs/view-logs/js/ViewLogs.js"></script>
{{/extend}}
