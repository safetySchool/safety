{{!< layout/default}} {{#extend "css" }} <style>
    </style>
    {{/extend}}
    <div class="content">
        <div class="card-head pb-4">
            <h4 class="card-title" style="margin-bottom: 0px;">
                <i class="icon-home position-left"></i>
                <span class="text-semibold">Bienvenido!</span>
            </h4>
        </div>

        <div class="card-body" style="padding: 0px;">
            <div id="carouselExampleIndicators" class="carousel slide carousel-fade mx-auto" data-ride="carousel">
                <ol class="carousel-indicators">
                    <li data-target="#carouselExampleIndicators" data-slide-to="0" class=""></li>
                    <li data-target="#carouselExampleIndicators" data-slide-to="1" class="active"></li>
                    <li data-target="#carouselExampleIndicators" data-slide-to="2" class=""></li>
                </ol>
                <div class="carousel-inner">
                    <div class="carousel-item active">
                        <img src="https://w0.peakpx.com/wallpaper/436/649/HD-wallpaper-macbook-pro-2023-abstract-colorful.jpg"
                            class="d-block w-100" alt="...">
                        <div class="carousel-caption d-none d-md-block">
                            <h5>TEXTO DE LA 1ERA IMAGEN</h5>
                            <p>PARRAFO DE LA 1ERA IMAGEN</p>
                        </div>
                    </div>
                    <div class="carousel-item">
                        <img src="https://w0.peakpx.com/wallpaper/583/502/HD-wallpaper-macbook-pro-2023-abstract-colorful.jpg"
                            class="d-block w-100" alt="...">
                        <div class="carousel-caption d-none d-md-block">
                            <h5>TEXTO DE LA 2DA IMAGEN</h5>
                            <p>PARRAFO DE LA 2DA IMAGEN</p>
                        </div>
                    </div>
                    <div class="carousel-item">
                        <img src="https://w0.peakpx.com/wallpaper/199/574/HD-wallpaper-macos-mojave-chroma-macos-mojave-macbook-pro-apple-computer-original-colorful-abstract.jpg"
                            class="d-block w-100" alt="...">
                        <div class="carousel-caption d-none d-md-block">
                            <h5>TEXTO DE LA 3ERA IMAGEN</h5>
                            <p>PARRAFO DE LA 3ERA IMAGEN</p>
                        </div>
                    </div>
                </div>
                <button class="carousel-control-prev" type="button" data-target="#carouselExampleIndicators"
                    data-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </button>
                <button class="carousel-control-next" type="button" data-target="#carouselExampleIndicators"
                    data-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </button>
            </div>
        </div>
    </div>
    {{#extend "js"}}
    {{/extend}}